#include <stdio.h>
int adj[10][10], visited[10];
void dfs(int v, int n) {
    visited[v] = 1;
    printf("%d ", v);
    for (int i = 0; i < n; i++) {
        if (adj[v][i] && !visited[i]) {
            dfs(i, n);
        }
    }
}
int main() {
    int n = 4;
    // 0-1, 0-2, 1-2, 2-3
    adj[0][1] = adj[1][0] = 1;
    adj[0][2] = adj[2][0] = 1;
    adj[1][2] = adj[2][1] = 1;
    adj[2][3] = adj[3][2] = 1;
    for (int i = 0; i < n; i++)
        visited[i] = 0;
    printf("DFS Traversal: ");
    dfs(0, n);
    return 0;
}
